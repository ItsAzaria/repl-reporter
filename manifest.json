{
    "name": "repl-reporter",
    "description": "Generate reports automatically for replit.com repositories",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "clipboardWrite"
    ],
    "host_permissions": [
        "https://discord.com/api/webhooks/*"
    ],
    "content_scripts": [
        {
          "matches": ["https://replit.com/*"],
          "js": ["repl-reporter.js"],
          "run_at": "document_end"
        }
      ]
  }